{
    "name": "backend",
    "version": "0.0.1",
    "description": "",
    "author": "Cute_Wisp",
    "private": true,
    "license": "UNLICENSED",
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "prettier",
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "prettier": "prettier --write \"src/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "sdk": "nestia sdk 'src/**/*.controller.ts' --out 'src/api'",
        "swagger": "nestia swagger 'src/**/*.controller.ts' --out './public/swagger.json'",
        "prepare": "ts-patch install && typia patch && husky"
    },
    "dependencies": {
        "@babel/core": "^7.25.7",
        "@nestia/e2e": "^0.7.0",
        "@nestia/fetcher": "^3.17.0",
        "@nestjs/common": "^10.4.4",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.4.4",
        "@nestjs/cqrs": "^10.2.7",
        "@nestjs/platform-express": "^10.4.4",
        "@nestjs/swagger": "^7.4.2",
        "@nestjs/typeorm": "10.0.2",
        "body-parser": "^1.20.3",
        "bs58": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cls-rtracer": "^2.6.3",
        "cross-fetch": "^4.0.0",
        "dotenv": "^16.4.5",
        "express-rate-limit": "^7.4.1",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "nest-winston": "^1.9.7",
        "object-assign": "^4.1.1",
        "pg": "^8.13.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "tsconfig-paths": "^4.2.0",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.20",
        "typeorm-transactional": "^0.5.0",
        "typia": "^6.11.1",
        "winston": "^3.15.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@nestia/benchmark": "^0.2.3",
        "@nestia/core": "^3.17.0",
        "@nestia/sdk": "^3.17.0",
        "@nestjs/cli": "^10.4.5",
        "@nestjs/schematics": "^10.1.4",
        "@nestjs/testing": "^10.4.4",
        "@swc/cli": "0.4.1-nightly.20240914",
        "@swc/core": "^1.7.35",
        "@types/eslint__js": "^8.42.3",
        "@types/helmet": "^4.0.0",
        "@types/jest": "29.5.13",
        "@types/node": "^22.7.5",
        "@types/xml2js": "^0.4.14",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "jest": "29.7.0",
        "lint-staged": "^15.2.10",
        "nestia": "^6.2.0",
        "node-mocks-http": "^1.16.1",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "supertest": "^7.0.0",
        "ts-jest": "29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.2.1",
        "ttypescript": "^1.5.15",
        "typescript": "^5.6.3",
        "typescript-eslint": "8.15.0"
    },
    "peerDependencies": {
        "webpack": "^5.90.3"
    }
}